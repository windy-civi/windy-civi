{"legislative_session": "2025_26", "identifier": "HB 184", "title": "Georgia Consumer Protection Towing Act; enact", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "description", "abstract": "A BILL to be entitled an Act to amend Titles 40 and 44 of the O.C.G.A., relating to motor vehicles and traffic and property, respectively, so as to establish the \"Georgia Consumer Protection Towing Act\"; to provide for applicability; to provide for consumer protections for emergency towing, owner requested towing, and private property towing; to provide for estimates and invoices; to provide for notice requirements; to provide for recording; to provide for the release of towed motor vehicles; to provide for fees; to provide for towing company certificate requirements; to provide for prohibitions; to provide for penalties and enforcement; to provide for rules and regulations; to provide for conforming changes; to provide for related matters; to provide for an effective date; to repeal conflicting laws; and for other purposes."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "House Hopper", "date": "2025-01-29", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["introduction"], "related_entities": []}, {"description": "House First Readers", "date": "2025-01-30", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "House Second Readers", "date": "2025-02-03", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-2"], "related_entities": []}], "sponsorships": [{"name": "Eddie Lumsden", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Eddie Lumsden\"}", "organization_id": null}, {"name": "Clint Crowe", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Cl