{"legislative_session": "104th", "identifier": "HB45", "title": "CONSUMER FRAUD-GROCERY COUPONS", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Consumer Fraud and Deceptive Business Practices Act. Provides that a grocery store that offers a digital coupon to consumers shall make available a corresponding paper coupon of equal value. Provides that the paper coupons shall be easily accessible at the service desk and may also be placed in other locations around the store. Provides that a grocery store that violates the requirement commits an unlawful practice within the meaning of the Act."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled with Clerk by Rep. Janet Yang Rohr", "date": "2024-12-11", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-01-09", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-01-09", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Assigned to Consumer Protection Committee", "date": "2025-02-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": []}, {"description": "Added Chief Co-Sponsor Rep. Norma Hernandez", "date": "2025-02-13", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Rule 19(a) / Re-referred to Rules Committee", "date": "2025-03-21", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Janet Yang Rohr", "classification": "primary", "entity_type": "person", "primary": true, "person_id