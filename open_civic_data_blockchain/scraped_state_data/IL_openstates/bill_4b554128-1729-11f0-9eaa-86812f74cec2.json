{"legislative_session": "104th", "identifier": "HB3693", "title": "TIF-COMPLETION", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Tax Increment Allocation Redevelopment Act in the Illinois Municipal Code. Provides that, if the ordinance approving the redevelopment project area was adopted on or after July 1, 2025, then the estimated dates of completion of the redevelopment project and the retirement of obligations issued to finance redevelopment project costs may not be later than December 31 of the year in which the payment to the municipal treasurer is to be made with respect to ad valorem taxes levied in the 15th (currently, 23rd) calendar year after the year in which the ordinance approving the redevelopment project area was adopted. Effective immediately."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with the Clerk by Rep. Maura Hirschauer", "date": "2025-02-07", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-02-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-02-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Assigned to Revenue & Finance Committee", "date": "2025-03-11", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": []}, {"description": "Rule 19(a) / Re-referred to Rules Committee", "date": "2025-03-21", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Maura Hirschauer", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"c