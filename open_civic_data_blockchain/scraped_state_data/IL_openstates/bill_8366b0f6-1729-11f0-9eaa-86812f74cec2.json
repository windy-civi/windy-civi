{"legislative_session": "104th", "identifier": "HR105", "title": "HOUSE RULES-AMEND", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["resolution"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends House Rule 41. Deletes a provision that allows a bill's principal sponsor or, in some circumstances, its chief co-sponsor to make a motion to have a note request deemed inapplicable. Deletes a provision that causes a note request to be automatically deemed inapplicable or, in some cases, applicable when certain conditions are met. Deletes a provision which states that a note request that is deemed inapplicable shall not be further considered and shall not prevent the bill from advancing."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with the Clerk by Rep. David Friess", "date": "2025-02-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-02-06", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "David Friess", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"David Friess\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/HR/PDF/10400HR0105.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=200425&DocName=10400HR0105&DocNum=105&DocTypeID=HR&LegID=160638&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/html"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://beta.ilga.gov/Legislation/BillStatus?DocNum=105&GAID=18&DocTypeID=HR&LegId=160638&SessionID=114", "note": ""}], "extras": {}, "_id": "