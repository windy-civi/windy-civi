{"legislative_session": "104th", "identifier": "HB2389", "title": "FOID-CARD VALIDITY LENGTH", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Firearm Owners Identification Card Act. Provides that if a Firearm Owner's Identification Card is issued to a holder under 21 years of age, then that Firearm Owner's Identification Card shall expire on the holder's 21st birthday or 10 years from the date of issuance, whichever is earlier. Provides that after the holder's 21st birthday, the validity period of the holder's Firearm Owner's Identification Card shall be 10 years."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with the Clerk by Rep. Terra Costa Howard", "date": "2025-01-31", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-02-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-02-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Terra Costa Howard", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"Terra Costa Howard\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/HB/PDF/10400HB2389.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=199896&DocName=10400HB2389&DocNum=2389&DocTypeID=HB&LegID=160108&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/html"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://beta