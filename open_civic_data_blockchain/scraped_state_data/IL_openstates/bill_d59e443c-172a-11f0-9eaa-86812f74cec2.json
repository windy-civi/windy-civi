{"legislative_session": "104th", "identifier": "SB2304", "title": "UNEMPLOYMENT INS-LABOR DISPUTE", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Unemployment Insurance Act. Provides that an individual shall be ineligible for benefits for a period totaling and not to exceed 2 weeks (rather than an individual shall be ineligible for benefits for any week) with respect to which it is found that his total or partial unemployment is due to a stoppage of work which exists because of a labor dispute at the factory, establishment, or other premises at which he is or was last employed. Provides that, after the 2-week period, the individual will be eligible for benefits."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with Secretary by Sen. Rachel Ventura", "date": "2025-02-07", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-02-07", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Assignments", "date": "2025-02-07", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "Added as Chief Co-Sponsor Sen. Ram Villivalam", "date": "2025-02-20", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "Added as Co-Sponsor Sen. Javier L. Cervantes", "date": "2025-03-06", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Rachel Ventura", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Rachel Ventura\"}", "organization_id": null}, {"name": "Ram Villivalam", "classification