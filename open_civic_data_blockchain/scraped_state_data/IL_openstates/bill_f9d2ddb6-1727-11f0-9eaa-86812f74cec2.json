{"legislative_session": "104th", "identifier": "HB1132", "title": "FIREARM DEALERS-FEE AMOUNTS", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Firearm Dealer License Certification Act. Provides that the fee amount for a certified licensee shall be based upon the number of firearms sold per year. Provides that if the certified licensee sells less than 150,000 firearms per year, the fee is $200. Provides that if the certified licensee sells 150,000 to 500,000 firearms per year, the fee is $500. Provides that if the certified licensee sells over 500,000 firearms per year, the fee is $1,000."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled with Clerk by Rep. Charles Meier", "date": "2025-01-03", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-01-09", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-01-09", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Charles Meier", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"Charles Meier\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/HB/PDF/10400HB1132.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=196634&DocName=10400HB1132&DocNum=1132&DocTypeID=HB&LegID=156868&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/html"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "htt