{"legislative_session": "104th", "identifier": "SB1577", "title": "TOBACCO STORES NEAR SCHOOL ACT", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Creates the Prohibition on Retail Tobacco Stores Near Schools Act. Provides that no county or municipality shall permit a retail tobacco store, as defined in Section 10 of the Smoke Free Illinois Act, to open an establishment within 100 feet of a school after the effective date of the Act. Provides that a county or municipality is not required to prohibit any establishment in existence prior to the effective date of the Act."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with Secretary by Sen. Mary Edly-Allen", "date": "2025-02-04", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-02-04", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Assignments", "date": "2025-02-04", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Mary Edly-Allen", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Mary Edly-Allen\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/SB/PDF/10400SB1577.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=200110&DocName=10400SB1577&DocNum=1577&DocTypeID=SB&LegID=160323&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/html"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://beta.ilga.gov/Legislati