{"legislative_session": "104th", "identifier": "HB3168", "title": "TIANEPTINE-RETAIL SALE PROHIB", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Creates the Retail Sale of Tianeptine Prohibition Act. Provides that beginning January 1, 2026, no person shall sell, offer to sell, or distribute in this State any product containing tianeptine. Provides that the prohibition does not apply to any product containing tianeptine that is lawfully dispensed or prescribed by a pharmacist or a health care professional. Provides that a violation is a business offense, punishable by a minimum fine of $1,500 for each violation. Defines \"health care professional\". Effective January 1, 2026."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with the Clerk by Rep. Jawaharial Williams", "date": "2025-02-06", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-02-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-02-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Assigned to Executive Committee", "date": "2025-03-11", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": []}, {"description": "Rule 19(a) / Re-referred to Rules Committee", "date": "2025-03-21", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Jawaharial Williams", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"Jawaharial Williams\"}", "organization_id": null}], "related_bills"