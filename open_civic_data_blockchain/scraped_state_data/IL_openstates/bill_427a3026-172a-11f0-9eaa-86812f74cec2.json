{"legislative_session": "104th", "identifier": "SB1210", "title": "SCH CD-FIREARM SAFETY COURSE", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the School Code. Provides that a school district may offer a firearm safety training course to students in grades 9 through 12, which may include instruction on hunting safety. Provides that if a school district offers the course, a student may not be required to take the course. Provides that if a school board intends to offer a firearm safety training course, it must develop a policy to implement the course and must publish that policy on the district's website."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with Secretary by Sen. Neil Anderson", "date": "2025-01-24", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-01-24", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Assignments", "date": "2025-01-24", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "Added as Co-Sponsor Sen. Dave Syverson", "date": "2025-02-11", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Neil Anderson", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Neil Anderson\"}", "organization_id": null}, {"name": "Dave Syverson", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Dave Syverson\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/docume