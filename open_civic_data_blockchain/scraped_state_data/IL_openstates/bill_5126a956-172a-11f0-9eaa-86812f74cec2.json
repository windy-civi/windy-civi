{"legislative_session": "104th", "identifier": "SB1320", "title": "AIRPORT AUTH-BD COMPENSATION", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Airport Authorities Act. Provides that, in airport authorities having a population of not more than 500,000, the compensation for services of any commissioner shall not exceed $300 (rather than $150) in any one month for services within the corporate limits of the authority or within a distance of 50 miles from the corporate limits of the authority. Provides that, in airport authorities having a population of more than 500,000, the compensation for services of any commissioner shall not exceed the sum of $25,000 (rather than $10,000) per annum."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with Secretary by Sen. Ram Villivalam", "date": "2025-01-28", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-01-28", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Assignments", "date": "2025-01-28", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Ram Villivalam", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Ram Villivalam\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/SB/PDF/10400SB1320.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=199051&DocName=10400SB1320&DocNum=1320&DocTypeID=SB&LegID=159262&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/htm