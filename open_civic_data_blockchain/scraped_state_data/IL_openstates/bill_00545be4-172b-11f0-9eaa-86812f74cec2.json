{"legislative_session": "104th", "identifier": "SB2623", "title": "VEH CD-SCHOOL-ACTIVITY BUS USE", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Illinois Vehicle Code. Provides that a multifunction school-activity bus may additionally be used for transportation to a college, university, student job site developed through a partnership with the school, a college or career fair, or an experience that the school determines will contribute to a student's college or career readiness. Amends the School Code to make conforming changes and to provide that a student in any of grades 7 through 12 (instead of grades 9 through 12) may be transported in a multifunction school-activity bus for specified purposes."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with Secretary by Sen. Laura M. Murphy", "date": "2025-03-04", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-03-04", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Assignments", "date": "2025-03-04", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Laura M. Murphy", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Laura M. Murphy\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/SB/PDF/10400SB2623.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=202990&DocName=10400SB2623&DocNum=2623&DocTypeID=SB&LegID=163109&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "medi