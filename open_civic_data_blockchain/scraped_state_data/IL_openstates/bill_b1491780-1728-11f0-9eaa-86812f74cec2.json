{"legislative_session": "104th", "identifier": "HB2514", "title": "PRESCRIBED BURNING-LIABILITY", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Illinois Prescribed Burning Act. Establishes the procedures required before conducting a prescribed burning. Provides that no landowner, agent of the landowner, or certified prescribed burn manager shall be liable for damage, injury, or loss caused by a prescribed burn or resulting smoke of a prescribed burn unless the landowner, agent of the landowner, or certified prescribed burn manager is proven to be grossly negligent. Effective immediately."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with the Clerk by Rep. Jeff Keicher", "date": "2025-02-03", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-02-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-02-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Jeff Keicher", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"Jeff Keicher\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/HB/PDF/10400HB2514.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=200129&DocName=10400HB2514&DocNum=2514&DocTypeID=HB&LegID=160342&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/html"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https