{"legislative_session": "104th", "identifier": "HB19", "title": "FOID&CRIM CD-FLAMETHROWER", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Firearm Owners Identification Card Act. Requires a person to have a currently valid Firearm Owner's Identification Card issued in his or her own name by the Illinois State Police in order to acquire or possess a flamethrower. Exempts certain military, law enforcement, and other specified persons. Defines flamethrower. Amends the Criminal Code of 2012. Provides that it is a Class 4 felony to deliver a flamethrower to a person, incidental to a sale, without withholding delivery of the flamethrower for at least 72 hours after application for its purchase has been made."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled with Clerk by Rep. Daniel Didech", "date": "2024-12-05", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-01-09", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-01-09", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Assigned to Judiciary - Criminal Committee", "date": "2025-02-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": []}, {"description": "Rule 19(a) / Re-referred to Rules Committee", "date": "2025-03-21", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Daniel Didech", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"Daniel Didech\"}", "organizati