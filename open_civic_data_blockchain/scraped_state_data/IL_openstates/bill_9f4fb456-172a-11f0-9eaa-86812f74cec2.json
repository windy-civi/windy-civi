{"legislative_session": "104th", "identifier": "SB1900", "title": "INCLUSIVE VENTURE INVESTMENT", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Creates the Inclusive Venture Investment Act. Provides that the State Treasurer shall create a Direct Matching Funds Program. Provides that the purpose of the program shall be to leverage State-managed funds for investments in minority-owned venture capital firms, minority-owned financial managers, and minority-led startups. Sets forth provisions for investment requirements and incentives; administration; transparency and reporting; oversight and compliance; confidentiality; and rulemaking. Effective July 1, 2026."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with Secretary by Sen. Elgie R. Sims, Jr.", "date": "2025-02-06", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-02-06", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Assignments", "date": "2025-02-06", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Elgie R. Sims, Jr.", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Elgie R. Sims, Jr.\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/SB/PDF/10400SB1900.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=200973&DocName=10400SB1900&DocNum=1900&DocTypeID=SB&LegID=161185&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/html"}], "date": "", "classificat