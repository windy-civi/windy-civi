{"legislative_session": "104th", "identifier": "HR171", "title": "MEDICAID FUNDING ACA", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["resolution"], "subject": [], "abstracts": [{"note": "", "abstract": "Urges the Illinois General Assembly to reconsider the repeal of the 2013 Medicaid funding trigger law and to protect those who may be impacted by federal Affordable Care Act (ACA) Medicaid funding cuts. Urges the Illinois General Assembly to take decisive budget actions in the healthcare arena and to aggressively advocate for Medicaid protection and expansion to ensure no vulnerable resident is left behind and no hospital, clinic, or emergency department is threatened out of existence."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with the Clerk by Rep. La Shawn K. Ford", "date": "2025-03-05", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-03-06", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "La Shawn K. Ford", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"La Shawn K. Ford\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/HR/PDF/10400HR0171.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=203120&DocName=10400HR0171&DocNum=171&DocTypeID=HR&LegID=163168&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/html"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://beta.ilga.gov/Legislation/BillStatus?DocNum=171&GAID=18&DocTypeID=HR&LegId=163168&SessionID=114", "note": ""}], "extras": {}, "_