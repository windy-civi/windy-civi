{"legislative_session": "104th", "identifier": "SR40", "title": "RETAIN NAME-STARVED ROCK", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["resolution"], "subject": [], "abstracts": [{"note": "", "abstract": "Supports the name Starved Rock State Park and recognizes its importance to the local community and its brand identity. Acknowledges the concerns raised by residents and local businesses regarding the potential renaming of Starved Rock State Park, and pledges to advocate for the preservation of the park's historic name. Encourages open dialogue and collaboration with relevant stakeholders, including Tribal Nations and Indigenous communities, to address concerns and explore ways to honor the park's heritage while respecting diverse perspectives."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with Secretary", "date": "2025-01-28", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["filing"], "related_entities": []}, {"description": "Referred to Assignments", "date": "2025-01-28", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Sue Rezin", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Sue Rezin\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/SR/PDF/10400SR0040.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=198854&DocName=10400SR0040&DocNum=40&DocTypeID=SR&LegID=159064&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/html"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://beta.ilga.gov/Legislation/BillStatus?DocNum=40&GAID=18&DocTypeID=SR&LegId=159064&SessionID=114", "note": ""}],