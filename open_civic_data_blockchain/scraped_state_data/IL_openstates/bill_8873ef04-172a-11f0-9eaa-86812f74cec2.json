{"legislative_session": "104th", "identifier": "SB1729", "title": "LABOR DISPUTES-BENEFITS", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Illinois Income Tax Act. Creates a deduction in an amount equal to any strike benefits paid to the taxpayer during the taxable year by a labor organization, union, or similar entity during a strike, work stoppage, or labor dispute. Amends the Unemployment Insurance Act. Provides that, if an individual's unemployment is due to a stoppage of work that exists because of a labor dispute, then the individual is ineligible for benefits for a period of not more than one week. Provides that, after the expiration of that one week period, the individual shall be eligible for benefits. Effective immediately."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with Secretary by Sen. Mike Porfirio", "date": "2025-02-05", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-02-05", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Assignments", "date": "2025-02-05", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "Added as Co-Sponsor Sen. Javier L. Cervantes", "date": "2025-03-06", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Mike Porfirio", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Mike Porfirio\"}", "organization_id": null}, {"name": "Javier L. Cervantes", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Javier L. Cervantes\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/SB/PDF/10400SB1729.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=200540&DocName=10400SB1729&DocNum=1729&DocTypeID=SB&LegID=160753&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/html"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://beta.ilga.gov/Legislation/BillStatus?DocNum=1729&GAID=18&DocTypeID=SB&LegId=160753&SessionID=114", "note": ""}], "extras": {}, "_id": "8873ef04-172a-11f0-9eaa-86812f74cec2"}