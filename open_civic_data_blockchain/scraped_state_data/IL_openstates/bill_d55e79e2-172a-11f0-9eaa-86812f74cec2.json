{"legislative_session": "104th", "identifier": "SB2302", "title": "BUSINESS ENTERPRISE-CERTIFY", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Business Enterprise for Minorities, Women, and Persons with Disabilities Act. Provides that certifications granted under the Act on or after the effective date of the amendatory Act shall be active for a minimum of 5 years. Provides that certified vendors shall inform the Program of any changes in the vendor's business status or ownership that may impact the vendor's certification status. Provides that Program staff shall inform the vendor no later than 6 months before the vendor's certification expires."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with Secretary by Sen. Cristina Castro", "date": "2025-02-07", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-02-07", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Assignments", "date": "2025-02-07", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Cristina Castro", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Cristina Castro\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/SB/PDF/10400SB2302.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=201919&DocName=10400SB2302&DocNum=2302&DocTypeID=SB&LegID=162131&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/html"}], "date": "", "classification": ""}